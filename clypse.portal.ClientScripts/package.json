{
  "name": "clypse-portal-clientscripts",
  "version": "1.0.0",
  "description": "Client-side JavaScript for Clypse Portal",
  "scripts": {
    "build": "node build.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "blazor",
    "webauthn",
    "cognito",
    "s3"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "json-summary"
    ]
  }
}
